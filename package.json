{
  "name": "rewer",
  "displayName": "rewer",
  "description": "branch code reviewer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "bin": {
    "rewer": "./bin/rewer.js"
  },
  "contributes": {
    "commands": [
      {
        "command": "rewer.helloRewer",
        "title": "Rewer: Show Changed Files"
      },
      {
        "command": "rewer.commitMsg",
        "title": "Rewer: Generate Commit Message"
      },
      {
        "command": "rewer.explainFile",
        "title": "Rewer: Explain File"
      },
      {
        "command": "rewer.dailyReport",
        "title": "Rewer: Daily Report"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.109.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.74.0"
  }
}
